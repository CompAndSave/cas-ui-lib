{
    "printWidth": 80,
    "semi": false,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "always",
    "singleAttributePerLine": true,
    "overrides": [
        {
            "files": "*.hbs",
            "options": {
                "singleQuote": false
            }
        }
    ],
    "plugins": [
        "prettier-plugin-tailwindcss"
    ]
}